{
	"name": "zef-dev/convoworks-pckg-forms",
	"type": "library",
	"description": "Contains conversational workflow elements for managing forms in the Convoworks framework.",
	"homepage": "https://github.com/zef-dev/convoworks-pckg-forms",
	"keywords": [
		"convoworks"
	],
	"license": "MIT",
	"version": "1.1.1",
	"require": {
		"php": "^7.2",
		"psr/log": "^1.1.0",
		"zef-dev/convoworks-core": "^0.22"
	},
	"autoload": {
		"psr-4": {
			"Convo\\Pckg\\Forms\\": "src/Convo/Pckg/Forms"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Convo\\": "tests/"
		}
	},
	"require-dev": {
		"phpunit/phpunit": "^8"
	},
	"config": {
		"platform": {
			"php": "7.2.5"
		},
		"sort-packages": true
	},
	"minimum-stability": "stable"
}